{
	"name": "lol-auto-accept",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"create-opk": "cd build && tar -acf ../dist-opk.v$npm_package_version.opk *",
		"build:overwolf": "npm run build && npm run create-opk"
	},
	"devDependencies": {
		"@overwolf/types": "^3.23.9",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.25.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/node": "^20.8.6",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^3.9.2",
		"daisyui-ntsd": "1.0.8",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"glob": "^10.3.10",
		"mdsvex": "^0.11.0",
		"nanostores": "^0.9.4",
		"overwolf-nanostores": "^1.0.2",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.1",
		"svelte-check": "^3.4.3",
		"svelte-markdown": "^0.4.0",
		"svelte-preprocess-markdown": "^2.7.3",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	}
}